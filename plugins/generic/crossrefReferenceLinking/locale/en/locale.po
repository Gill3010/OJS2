msgid "plugins.generic.crossrefReferenceLinking.displayName"
msgstr "Crossref Reference Linking"

msgid "plugins.generic.crossrefReferenceLinking.description"
msgstr "This plugin submits article references to Crossref when a DOI is registered and automatically displays DOIs for references found in Crossref's database."

msgid "plugins.generic.crossrefReferenceLinking.description.long"
msgstr "This plugin will automatically add the extracted article references to the DOI registration with Crossref. Crossref will then automatically check if any DOIs can be found for the submitted references. Using the scheduled task the plugin will then check for the found references DOIs."

msgid "plugins.generic.crossrefReferenceLinking.description.note"
msgstr "If you change an article's references after registering the DOI, you will need to register the article's DOI again and check again for the article references' DOIs."

msgid "plugins.generic.crossrefReferenceLinking.description.requirements"
msgstr "This plugin requires an article to be assigned a Crossref DOI and this DOI must be deposited with Crossref from within OJS. Article references can be provided in the References section in the Publication tab of each submission."

msgid "plugins.generic.crossrefReferenceLinking.settings.form.crossrefSettings"
msgstr "Crossref Settings"

msgid "plugins.generic.crossrefReferenceLinking.settings.form.crossrefSettings.required"
msgstr "Crossref username and password must be configured:"

msgid "plugins.generic.crossrefReferenceLinking.settings.form.submissionSettings"
msgstr "Submission Metadata Settings"

msgid "plugins.generic.crossrefReferenceLinking.settings.form.submissionSettings.required"
msgstr "Submission references metadata must be enabled:"

msgid "plugins.generic.crossrefReferenceLinking.settings.form.requirements.ok"
msgstr "You're all set up!"

msgid "plugins.generic.crossrefReferenceLinking.citationsFormActionName"
msgstr "Check Crossref DOIs"

msgid "plugins.generic.crossrefReferenceLinking.senderTask.name"
msgstr "CrossrefReferenceLinking automatic registration task"
